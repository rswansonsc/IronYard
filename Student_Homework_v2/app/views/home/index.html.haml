%h1 Iron Yard Homework Tracker
= link_to "Create Location", new_location_path
= link_to "List Locations", locations_path(@location)
%hr
- @locations.each do |location|
 = link_to location.name_l, location_path(location)
 = link_to "Edit Location", edit_location_path(location)
 = link_to "Add New Location", new_location_path(location)
 %br

%hr
- @courses.each do |course|
 = link_to course.name_course, course_path(course)
 = link_to "Edit Course", edit_course_path(course)
 = link_to "Add New Course", new_course_path(course)
 %br

%hr
- @assignments.each do |assignment|
 = link_to assignment.name_a, assignment_path(assignment)
 = link_to "Edit Assignment", edit_assignment_path(assignment)
 = link_to "Add New Assignment", new_assignment_path(assignment)


 
 